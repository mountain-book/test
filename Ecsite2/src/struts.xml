<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<constant name="struts.devMode" value="true"/>
	<package name="com.internousdev.ecsite2.action" extends="struts-default">
		<!-- HomeAction -->
		<action name="HomeAction" class="com.internousdev.ecsite2.action.HomeAction" method="execute">
		<result name="login">login.jsp</result>
		<result name="success">buyItemList.jsp</result>
		</action>

		<!-- LoginAction -->
		<action name="LoginAction" class="com.internousdev.ecsite2.action.LoginAction" method="execute">
		<result name="success">buyItemList.jsp</result>
		<result name="error">login.jsp</result>
		<result name="manager">manager.jsp</result>
		</action>

		<!-- GoHomeAction -->
		<action name="GoHomeAction" class="com.internousdev.ecsite2.action.GoHomeAction" method="execute">
		<result name="success">home.jsp</result>
		</action>

		<!-- LogoutAction -->
		<action name="LogoutAction" class="com.internousdev.ecsite2.action.LogoutAction" method="execute">
		<result name="success">home.jsp</result>
		</action>

		<!-- UserCreateAction -->
		<action name="UserCreateAction" class="com.internousdev.ecsite2.action.UserCreateAction" method="execute">
		<result name="success">userCreate.jsp</result>
		</action>

		<!-- UserCreateConfirmAction -->
		<action name="UserCreateConfirmAction" class="com.internousdev.ecsite2.action.UserCreateConfirmAction" method="execute">
		<result name="success">userCreateConfirm.jsp</result>
		<result name="error">userCreate.jsp</result>
		</action>

		<!-- UserCreateCompleteAction -->
		<action name="UserCreateCompleteAction" class="com.internousdev.ecsite2.action.UserCreateCompleteAction" method="execute">
		<result name="success">userCreateComplete.jsp</result>
		</action>

		<!-- BuyItemAction -->
		<action name="BuyItemAction" class="com.internousdev.ecsite2.action.BuyItemAction" method="execute">
		<result name="success">buyItemConfirm.jsp</result>
		</action>

		<!-- BuyItemConfirmAction -->
		<action name="BuyItemConfirmAction" class="com.internousdev.ecsite2.action.BuyItemConfirmAction" method="execute">
		<result name="success">buyItemComplete.jsp</result>
		</action>

		<!-- MyPageAction -->
		<action name="MyPageAction" class="com.internousdev.ecsite2.action.MyPageAction" method="execute">
		<result name="success">myPage.jsp</result>
		<result name="error">error.jsp</result>
		</action>

		<!-- UserEditAction -->
		<action name="UserEditAction" class="com.internousdev.ecsite2.action.UserEditAction" method="execute">
		<result name="success">userEdit.jsp</result>
		</action>

		<!-- BuyItemEditAction -->
		<action name="BuyItemEditAction" class="com.internousdev.ecsite2.action.BuyItemEditAction" method="execute">
		<result name="success">buyItemEdit.jsp</result>
		</action>

		<!-- GoManagerAction -->
		<action name="GoManagerAction" class="com.internousdev.ecsite2.action.GoManagerAction" method="execute">
		<result name="success">manager.jsp</result>
		</action>

		<!-- BuyItemAddtionAction -->
		<action name="BuyItemAdditionAction" class="com.internousdev.ecsite2.action.BuyItemAdditionAction" method="execute">
		<result name="success">buyItemAddition.jsp</result>
		</action>

		<!-- BuyItemAdditionConfirmAction -->
		<action name="BuyItemAdditionConfirmAction" class="com.internousdev.ecsite2.action.BuyItemAdditionConfirmAction" method="execute">
		<result name="success">buyItemAdditionConfirm.jsp</result>
		<result name="error">buyItemAddition.jsp</result>
		</action>

		<!-- BuyItemAdditionCompleteAction -->
		<action name="BuyItemAdditionCompleteAction" class="com.internousdev.ecsite2.action.BuyItemAdditionCompleteAction" method="execute">
		<result name="success">buyItemAdditionComplete.jsp</result>
		</action>

	</package>
</struts>